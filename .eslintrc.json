{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/label-has-associated-control": [1, { "assert": "either" }],
    "react/jsx-no-bind": "off"
  }
}
